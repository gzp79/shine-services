{
  "service": {
    "allowedOrigins": ["^https:\\/\\/([a-zA-Z0-9-]+\\.)+scytta\\.com(:\\d+)?$"],
    "allowedHosts": {
      "default": ["^cloud\\.scytta\\.com$"],
      "ws": ["^ws\\.scytta\\.com$"]
    },
    "fullProblemResponse": false,
    "sessionTtl": 1800
  },
  "telemetry": {
    "allowReconfigure": true,
    "enableConsoleLog": true,
    "defaultLevel": "INFO,shine_builder=TRACE",
    "metrics": {
      "type": "prometheus"
    },
    "tracing": {
      "type": "none"
    }
  }
}
