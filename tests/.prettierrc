{
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ],
    "useTabs": false,
    "tabWidth": 4,
    "singleQuote": true,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "semi": true,
    "trailingComma": "none",
    "printWidth": 110,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^\\$(.*)",
        "^[../]",
        "^[./]"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}